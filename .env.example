# CitySense environment variables
# Copy to .env and fill in values. See citysense.toml.example for file-based config.

# Pilot country (az|fi|se|dk|no)
# CITYSENSE_PILOT_COUNTRY=fi

# Vector store (Qdrant)
# CITYSENSE_VECTOR_STORE_URL=http://localhost:6333

# LLM (LiteLLM; Anthropic for default model)
# ANTHROPIC_API_KEY=

# Mapillary API v4
# CITYSENSE_MAPILLARY_ACCESS_TOKEN=

# Copernicus Data Space Ecosystem
# CITYSENSE_CDSE_CLIENT_ID=
# CITYSENSE_CDSE_CLIENT_SECRET=

# Sentinel Hub Process API (optional)
# CITYSENSE_SENTINEL_HUB_CLIENT_ID=
# CITYSENSE_SENTINEL_HUB_CLIENT_SECRET=
